<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE tiles-definitions PUBLIC "-//Apache Software Foundation//DTD Tiles Configuration 2.1//EN" "http://tiles.apache.org/dtds/tiles-config_2_1.dtd">
           
<tiles-definitions>
	<!-- Main Template -->
	<definition name="template" template="/WEB-INF/view/jsp/templates/mainTemplate.jsp">
		<put-attribute name="top" value="/WEB-INF/view/jsp/templates/topTemplate.jsp" />
		<put-attribute name="notes" value="/WEB-INF/view/jsp/templates/notesTemplate.jsp" />
		<put-attribute name="searchbar" value="/WEB-INF/view/jsp/templates/searchbarTemplate.jsp" />
		<put-attribute name="footer" value="/WEB-INF/view/jsp/templates/footerTemplate.jsp" />
		<put-attribute name="showFooter" cascade="true" value="true" />
		<put-attribute name="showSearchBar" cascade="true" value="false" />
		<put-attribute name="fluidLayout" cascade="true" value="true" />
		<put-list-attribute name="styles">
			<add-attribute type="string" value="bootstrap.css" />
			<add-attribute type="string" value="bootstrap-responsive.css" />
			<add-attribute type="string" value="application.css" />
			<add-attribute type="string" value="bootstrap-customization.css" />
			<add-attribute type="string" value="bootstrap-modal.css" />
        </put-list-attribute>
		<put-list-attribute name="scripts">
			<add-attribute type="string" value="jquery-1.8.2.js" />
			<add-attribute type="string" value="bootstrap.js" />
			<add-attribute type="string" value="util.js" />
			<add-attribute type="string" value="modalMessage.js" />
			<add-attribute type="string" value="spin.js" />
			<add-attribute type="string" value="annotationViewer.js" />
            <add-attribute type="string" value="MessageArea.js" />
        </put-list-attribute>
	</definition>
	
	<!-- Home -->
	<definition name="home" extends="template">
		<put-attribute name="title" value="Homepage" />
		<put-attribute name="section" value="home" />
		<put-attribute name="content" value="/WEB-INF/view/jsp/home.jsp" />
	</definition>
	
	<!-- Schema -->
	<definition name="schema" extends="template">
		<put-attribute name="section" value="schema" />
	</definition>
	
	<definition name="schema/list" extends="schema">
		<put-attribute name="title" value="List Schemas" />
		<put-attribute name="content" value="/WEB-INF/view/jsp/schema/list.jsp" />
		<put-list-attribute name="scripts" inherit="true">
			<add-attribute type="string" value="jquery.ui.widget.js" />
            <add-attribute type="string" value="jquery.iframe-transport.js" />
            <add-attribute type="string" value="jquery.fileupload-5.19.3.js" />
            <add-attribute type="string" value="modelViewRefresher.js" />
            <add-attribute type="string" value="form.js" />
            <add-attribute type="string" value="schema.js" />
           
        </put-list-attribute>
	</definition>
	
	<definition name="schema/edit" extends="schema">
		<put-attribute name="title" value="Edit Schema" />
		<put-attribute name="content" value="/WEB-INF/view/jsp/schema/edit.jsp" />
	</definition>
	
	<!-- Mapping -->
	<definition name="mapping" extends="template">
		<put-attribute name="section" value="mapping" />
	</definition>
	
	<definition name="mapping/list" extends="mapping">
		<put-attribute name="title" value="List Crosswalks" />
		<put-attribute name="content" value="/WEB-INF/view/jsp/mapping/list.jsp" />
		
		<put-list-attribute name="scripts" inherit="true">
		    <add-attribute type="string" value="form.js" />
		    <add-attribute type="string" value="modelViewRefresher.js" />
            <add-attribute type="string" value="mapping.js" />
            <add-attribute type="string" value="ajax.js" />
        </put-list-attribute>
	</definition>
	<definition name="mapping/edit" extends="mapping">
		<put-attribute name="title" value="Edit Crosswalk" />
		<put-attribute name="content" value="/WEB-INF/view/jsp/mapping/edit.jsp" />
	</definition>
	

	<definition name="mapping/analyze" extends="mapping">
		<put-attribute name="title" value="Analyze Crosswalk" />
		<put-attribute name="content" value="/WEB-INF/view/jsp/mapping/analyze.jsp" />
		<put-list-attribute name="styles"  inherit="true">
			<add-attribute type="string" value="mapping.css" />
			<add-attribute type="string" value="jquery-ui-1.10.0.custom.css" />
        </put-list-attribute>
		<put-list-attribute name="scripts" inherit="true">
			<add-attribute type="string" value="jquery-ui-1.10.0.custom.min.js" />
			<add-attribute type="string" value="form.js" />
			<add-attribute type="string" value="mappingUI.js" />
			<add-attribute type="string" value="mapping/tmpControl.js" />
			<add-attribute type="string" value="mapping/Area.js" />
			<add-attribute type="string" value="mapping/Array.js" />
			<add-attribute type="string" value="mapping/CanvasRenderingContext2D.js" />
			<add-attribute type="string" value="mapping/Connection.js" />
			<add-attribute type="string" value="mapping/Connector.js" />
			<add-attribute type="string" value="mapping/ContainerUndoUnit.js" />
			<add-attribute type="string" value="mapping/ContentChangedUndoUnit.js" />
			<add-attribute type="string" value="mapping/Cursors.js" />
			<add-attribute type="string" value="mapping/DeleteConnectionUndoUnit.js" />
			<add-attribute type="string" value="mapping/DeleteElementUndoUnit.js" />
			<add-attribute type="string" value="mapping/Element.js" />
			<add-attribute type="string" value="mapping/ElementTemplate.js" />
			<add-attribute type="string" value="mapping/Function.js" />
			<add-attribute type="string" value="mapping/Graph.js" />
			<add-attribute type="string" value="mapping/InsertConnectionUndoUnit.js" />
			<add-attribute type="string" value="mapping/InsertElementUndoUnit.js" />
			<add-attribute type="string" value="mapping/Point.js" />
			<add-attribute type="string" value="mapping/Rectangle.js" />
			<add-attribute type="string" value="mapping/Selection.js" />
			<add-attribute type="string" value="mapping/SelectionUndoUnit.js" />
			<add-attribute type="string" value="mapping/Tracker.js" />
			<add-attribute type="string" value="mapping/TransformUndoUnit.js" />
			<add-attribute type="string" value="mapping/UndoService.js" />
			<add-attribute type="string" value="mapping/HierarchyConnection.js" />
			<add-attribute type="string" value="mapping/MappingConnection.js" />
			<add-attribute type="string" value="mapping/Expander.js" />
			<add-attribute type="string" value="mapping/HNavBar.js" />
        </put-list-attribute>
	</definition>
	
	
	<!-- Admin -->
	<definition name="admin" extends="template">
		<put-attribute name="section" value="admin" />
	</definition>
	
	<definition name="admin/log" extends="admin">
		<put-attribute name="title" value="Administration - Debug Log" />
		<put-attribute name="content" value="/WEB-INF/view/jsp/admin/log.jsp" />
		<put-list-attribute name="scripts" inherit="true">
            <add-attribute type="string" value="admin.js" />
        </put-list-attribute>
	</definition>	
	<definition name="admin/userlog" extends="admin">
		<put-attribute name="title" value="Administration - User Log" />
		<put-attribute name="content" value="/WEB-INF/view/jsp/admin/userlog.jsp" />
		<put-list-attribute name="scripts" inherit="true">
            <add-attribute type="string" value="admin.js" />
        </put-list-attribute>
	</definition>	
	
	
	<!-- Auth -->
	<definition name="admin/auth" extends="template">
		<put-attribute name="section" value="auth" />
	</definition>	
	<definition name="admin/auth/management" extends="admin/auth">
		<put-attribute name="title" value="Authorization Management" />
		<put-attribute name="content" value="/WEB-INF/view/jsp/admin/auth/admin.jsp" />
		<put-list-attribute name="scripts" inherit="true">
			<add-attribute type="string" value="util.js" />
        </put-list-attribute>
	</definition>	
	<definition name="admin/auth/management/edit" extends="admin/auth">
		<put-attribute name="fluidLayout" cascade="true" value="false" />
		<put-attribute name="title" value="Authorization Management" />
		<put-attribute name="content" value="/WEB-INF/view/jsp/admin/auth/edit_role.jsp" />
	</definition>	
	
	
	<definition name="auth/saml/metadata" extends="admin/auth">
		<put-attribute name="fluidLayout" cascade="true" value="false" />
	</definition>		
	<definition name="auth/saml/metadata/list" extends="auth/saml/metadata">
		<put-attribute name="title" value="SAML Metadata List" />
		<put-attribute name="content" value="/WEB-INF/view/jsp/auth/saml/metadata/metadataList.jsp" />
	</definition>	
	<definition name="auth/saml/metadata/provider" extends="auth/saml/metadata">
		<put-attribute name="title" value="SAML Metadata Provider" />
		<put-attribute name="content" value="/WEB-INF/view/jsp/auth/saml/metadata/providerView.jsp" />
	</definition>	
	<definition name="auth/saml/metadata/generate" extends="auth/saml/metadata">
		<put-attribute name="title" value="Generate SAML Metadata" />
		<put-attribute name="content" value="/WEB-INF/view/jsp/auth/saml/metadata/metadataGenerator.jsp" />
	</definition>	
	<definition name="auth/saml/metadata/view" extends="auth/saml/metadata">
		<put-attribute name="title" value="View SAML Metadata" />
		<put-attribute name="content" value="/WEB-INF/view/jsp/auth/saml/metadata/metadataView.jsp" />
	</definition>	
				
	
	<!-- User -->
	<definition name="user" extends="template">
		<put-attribute name="section" value="user" />
		<put-attribute name="fluidLayout" cascade="true" value="false" />
	</definition>
	<definition name="user/profile" extends="user">
		<put-attribute name="title" value="User Profile" />
		<put-attribute name="content" value="/WEB-INF/view/jsp/user/profile.jsp" />
	</definition>
	<definition name="user/redirect" extends="user">
		<put-attribute name="title" value="Complete your use profile" />
		<put-attribute name="content" value="/WEB-INF/view/jsp/user/redirect.jsp" />
	</definition>
			
	<!-- User -->
	<definition name="error" extends="template">
		<put-attribute name="section" value="error" />
		<put-attribute name="fluidLayout" cascade="true" value="false" />
	</definition>
	
	<definition name="error/userCredentialsException" extends="error">
		<put-attribute name="title" value="Error processing your user credentials" />
		<put-attribute name="content" value="/WEB-INF/view/jsp/error/userCredentialsException.jsp" />
	</definition>
	
	<definition name="error/exception" extends="error">
		<put-attribute name="title" value="Error" />
		<put-attribute name="content" value="/WEB-INF/view/jsp/error/exception.jsp" />
	</definition>
	
	<definition name="error/unhandled" extends="error">
		<put-attribute name="title" value="Error" />
		<put-attribute name="content" value="/WEB-INF/view/jsp/error/unhandledException.jsp" />
	</definition>
				
